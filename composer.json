{
  "name": "arraypress/wp-page-utils",
  "version": "1.0.0",
  "description": "Dead simple page detection and registration for WordPress plugins",
  "type": "library",
  "license": "GPL-2.0-or-later",
  "homepage": "https://github.com/arraypress/wp-page-utils",
  "authors": [
    {
      "name": "David Sherlock",
      "homepage": "https://arraypress.com/"
    }
  ],
  "require": {
    "php": ">=7.4"
  },
  "autoload": {
    "psr-4": {
      "ArrayPress\\PageUtils\\": "src/"
    },
    "files": [
      "src/Functions.php"
    ]
  },
  "config": {
    "optimize-autoloader": true,
    "sort-packages": true,
    "preferred-install": "dist"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "support": {
    "issues": "https://github.com/arraypress/wp-page-utils/issues",
    "source": "https://github.com/arraypress/wp-page-utils"
  },
  "keywords": [
    "wordpress",
    "wp",
    "pages",
    "page-detection",
    "page-registration",
    "shortcode",
    "conditional-logic",
    "settings",
    "arraypress"
  ]
}